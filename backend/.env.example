# Environment Configuration
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Broker Configuration
# Set ACTIVE_BROKER to either "oanda" or "fxcm" to enable market data ingestion
ACTIVE_BROKER=oanda

# OANDA Configuration (required if ACTIVE_BROKER=oanda)
OANDA_API_URL=https://api-fxpractice.oanda.com
OANDA_API_KEY=your_oanda_api_key
OANDA_ACCOUNT_ID=your_oanda_account_id

# FXCM Configuration (required if ACTIVE_BROKER=fxcm)
FXCM_API_URL=https://api.fxcm.com
FXCM_ACCESS_TOKEN=your_fxcm_access_token